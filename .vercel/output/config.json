{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		}
	],
	"images": {
		"sizes": [
			320,
			640,
			768,
			1024,
			1280,
			1536
		],
		"formats": [
			"image/webp",
			"image/avif"
		],
		"domains": [],
		"remotePatterns": []
	}
}